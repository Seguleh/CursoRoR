<h1>Users</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Lastname</th>
			<th>Email</th>
			<th>Birthday</th>
			<th>Created at</th>
			<th>Updated at</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.name %></td>
				<td><%= user.lastname %></td>
				<td><%= user.email %></td>
				<td><%= user.birthday %></td>
				<td><%= user.created_at %></td>
				<td><%= user.updated_at %></td>
			</tr>
		<% end %>
	</tbody>
</table>